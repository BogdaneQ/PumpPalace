@model IEnumerable<PumpPalace.Models.Customer>

@{
    ViewData["Title"] = "User Management";
}

<h2>User Management</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Username</th>
            <th>Email</th>
            <th>Is Admin</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.Username</td>
                <td>@user.Email</td>
                <td>@user.IsAdmin</td>
            </tr>
        }
    </tbody>
</table>
